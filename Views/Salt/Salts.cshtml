@model IEnumerable<Salt>

@{
    ViewData["Title"] = "Salt info";
}

<section>
    <h3>List of Salts</h3>
    <ul>
        @foreach (var salt in @Model)
        {
            <section>
                <li>@salt.Name</li>
                <li>@salt.PartitionKey</li>
            </section>
        }
        HELLLOOOOOO!
    </ul>
</section>
